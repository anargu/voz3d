
:host
    position absolute
    top 76px
    // margin 2px 0px
    height auto
    z-index 3
    width 100%

.search-container
    width 100%
    background #fff
    -webkit-box-shadow: 2px 2px 22px -2px rgba(227,227,227,0.77)
    -moz-box-shadow: 2px 2px 22px -2px rgba(227,227,227,0.77)
    box-shadow: 2px 2px 22px -2px rgba(227,227,227,0.77)

.wrapper
    max-width 800px
    margin-left auto
    margin-right auto
    
ul
    list-style-type none
    padding 2px 0px
    margin-top 0px
    max-height: 400px
    overflow: auto
li
    font-size: 20px
    padding 8px 8px
li:hover
    background #81cfef

.title
    padding 8px
.string
    font-weight bold
    color #000
.no-results
    text-align center
    padding 8px

